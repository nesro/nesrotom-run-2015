class A {
  var x
  A(y) {
    x = y
  }
  append(str) {
    return str + x
  }
}

class Main {
  static ch(i) {
    if (i % 2 == 0) {
      return "a"
    } else {
      return "b"
    }
  }

  static main(args) {
    var i
    var x = ""
    var a = new A("x")
    var tmp
    for (i = 0; i < 1000; i++) {
      tmp = new A(ch(i))
      x = x + ch(i)
      x = a.append(x)
      x = tmp.append(x)
    }
    write x
    write "\n"
  }
}
