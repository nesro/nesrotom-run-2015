class Prime {
	static const n = 100, sqrtn = 10
	var i, j, isprime[2..n+1*3]

	Prime() {
		for (i = 2 to n) isprime[i] = 1

		// The sieve of Eratosthenes
		//for (i = 2 to sqrtn)
		for (i = 2; i <= sqrtn; i++) {
			if (isprime[i] == 1) {
				j = i * i
				while (j <= n) {
					isprime[j] = 0
					j = j + i
				}
			}
		}
		/*
			Long
			comment
			...
		*/

		var prime[0..n]
		j = 0
		for (i = 2 to n)
			if (isprime[i] == 1) {
				prime[j] = i
				j = j + 1
			}

		//for (i = 0 to j - 1) write prime[i]
		for (i = 0; i != j; i++) write prime[i]

		//write 10 % 4
	}
}

