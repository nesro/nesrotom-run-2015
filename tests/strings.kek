class Main {
	static main(args) {
		var tok = args[0].split("/")
		var i, len = tok.length()

		for (i = 0; i < len; i++) {
			write tok[i]; write "\n"
		}

		if (tok[len - 1] == "strings.kexe") {
			write "Program se jmenuje strings.kexe.\n"
		}
		var lt_len = tok[len - 1].length()
		if (tok[len - 1][lt_len - 1] == "e") {
			write "Posledni pismeno nazvu programu je \"e\".\n"
		}
	}
}
